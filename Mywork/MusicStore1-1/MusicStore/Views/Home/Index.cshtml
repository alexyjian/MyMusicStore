@{
    ViewBag.Title = "首页";
}

<div id="promotion"></div>

<h3><em>新歌</em>上架</h3>
<div>
    <ul id="album-list">
        @foreach(var item in Model)
        {
            <li>
                <a href="#">
                    <img alt="@item.Tite" src="@item.AlbumArtUrl" />
                    <span>@item.Artist.Name</span>.<span>@item.Tite</span>
                </a>
            </li>
        }
    </ul>
</div>
